<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".base.mvp.MoreModelMvpActivity">

    <Button
            android:id="@+id/tv_test_load_presenter1"
            android:layout_marginTop="32dp"
            android:layout_marginLeft="32dp"
            android:layout_marginRight="32dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:text="加载presenter1数据"
            android:layout_width="match_parent" android:layout_height="wrap_content"/>


    <Button
            android:id="@+id/tv_test_load_model_1"
            android:layout_marginTop="12dp"
            android:layout_marginLeft="32dp"
            android:layout_marginRight="32dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_test_load_presenter1"
            app:layout_constraintLeft_toLeftOf="@+id/tv_test_load_presenter1"
            app:layout_constraintRight_toRightOf="@+id/tv_test_load_presenter1"
            android:text="加载model1数据"
            android:layout_width="match_parent" android:layout_height="wrap_content"/>
    <Button
            android:id="@+id/tv_test_load_model_2"
            android:layout_marginTop="12dp"
            android:layout_marginLeft="32dp"
            android:layout_marginRight="32dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_test_load_model_1"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:text="加载model2数据"
            android:layout_width="match_parent" android:layout_height="wrap_content"/>

    <Button
            android:id="@+id/tv_test_load_model_3"
            android:layout_marginTop="12dp"
            android:layout_marginLeft="32dp"
            android:layout_marginRight="32dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_test_load_model_2"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:text="加载model3数据"
            android:layout_width="match_parent" android:layout_height="wrap_content"/>

    <TextView
            android:id="@+id/tv_test_result_show"
            android:layout_marginTop="32dp"
            android:layout_marginLeft="32dp"
            android:layout_marginRight="32dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_test_load_model_3"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:hint="显示结果:"
            android:layout_width="match_parent" android:layout_height="wrap_content"/>
</androidx.constraintlayout.widget.ConstraintLayout>